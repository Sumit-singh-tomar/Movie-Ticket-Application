<style>
    .maincontainer{
        height: 100vh;
        width: 100vw;
        background: gainsboro;
        overflow: hidden;
    }
    .box{
        height: auto;
        width:100vw;
        background: white;
        display: flex;
    }
    .companyname
    {
        width:20%;background-color:darkkhaki;display:flex;justify-content: center;font-size: 24;
    }
    .logout{
        width:10%;background-color:white;display: flex;justify-content: center;align-items: center;margin-left: auto;
    }
    .sideBarStyle{
        width: 85%;
        height: 95vh;
        background-color: aliceblue
    }
    .userimage{
        width:99%;display:flex;justify-content:center;
    }
    .usertext{
        width:99%;background-color: antiquewhite;display: flex;justify-content: center;margin-top: 5px;overflow:hidden;
    }
    .optionstext{
        width:99%;font-weight: 600;margin-top:10px;font-size:large;display:flex;justify-content:center;align-items: center;
    }
</style>
<html>
    <head>
        <title>Movie Interface Page</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script> 
      
    </head>
    <body>
        <div class="maincontainer" style="font-family: Consolas;" >
            <div class="box">
                <div class="companyname">Zombex Studio</div>
            </div>


        <div class="row">
            <div class="col-sm-3">
                
            <div class="sideBarStyle">
                <div class="userimage"><img src="/images/Inkedoriginal_LI.jpg" width="50%" style="border-radius:60px;margin-top: 20px;"></div>
                <div class="usertext"><%=data.adminname%></div>
                <div class="usertext"><%=data.emailid%></div>
                <div class="usertext">+91 <%=data.mobileno%></div>
                <div class="optionstext" style="margin-top:30px;"><a href="/movie/movieinterface" target="mw" style="color:black;text-decoration: none;display:flex;justify-content:center;align-items: center;"><img src="/images/addmovie.png" style="margin-right:3px;" width="13%">Add Movie</a></div>
                <div class="optionstext"><a href="/movie/display_records" target="mw" style="color:black;text-decoration: none;display:flex;justify-content:center;align-items: center;"><img src="/images/vieweye.png" width="10%" style="margin-right:3px;">View Records</a></div>
                <div class="optionstext"><a href="/movie/view_all_records" target="_blank" style="color:black;text-decoration: none;display:flex;justify-content:center;align-items: center;"><img src="/images/ticket.png" width="10%" style="margin-right:3px;">Book Ticket</a></div>
                <div class="optionstext"><a href="/admin/logout" style="color:black;text-decoration: none;display:flex;justify-content:center;align-items: center;"><img src="/images/logout.png" target="mw" width="8%" style="margin-right:3px;">Logout</a></div>
            </div>
            </div>

            <div class="col-sm-9">
                <iframe name="mw" width="99%" height="99%">
                </iframe> 
           </div>

        </div>
        </div>
</html>